{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "rules": {
        "no-unused-vars": 1,
        "no-var": 2,
        "constructor-super": 2,
        "no-const-assign": 2,
        "no-duplicate-imports": 2,
        "no-this-before-super": 2,
        "no-console": 0,
        "no-extra-boolean-cast": 0,
        "default-case": 1,
        "eqeqeq": 1,
        "no-multi-spaces": 1
    },
    "globals": {
        "arguments": true
      }
}
